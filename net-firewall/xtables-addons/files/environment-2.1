        ;;
        *)
            operator="-eq"
        ;;
    esac;
    [[ $# -gt 3 ]] && die "Error in kernel-2_kernel_is(): too many parameters";
    : $(( test = (KV_MAJOR << 16) + (KV_MINOR << 8) + KV_PATCH ));
    : $(( value = (${1:-${KV_MAJOR}} << 16) + (${2:-${KV_MINOR}} << 8) + ${3:-${KV_PATCH}} ));
