    eend $?;
    [ "$?" != 0 ] && die;
    ebegin "checking ZLIB_DEFLATE";
    linux_chkconfig_builtin CONFIG_ZLIB_DEFLATE;
    eend $?;
    [ "$?" != 0 ] && die;
    local LINENO_START;
    local LINENO_END;
    local SYMBOLS;
